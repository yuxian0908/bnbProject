	<script type="text/javascript" src="/visitors/visitors.client.module.js"></script>
	<script type="text/javascript" src="/visitors/controllers/visitors.client.controller.js"></script>
	<script type="text/javascript" src="/visitors/config/visitors.client.routes.js"></script>
	<script type="text/javascript" src="/visitors/services/visitors.client.service.js"></script>
